{
  "name": "svg-to-gif-electron",
  "version": "1.0.0",
  "description": "Convert animated (CSS/SMIL/JavaScript) SVG to GIF or WebP with configurable parameters (Electron).",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build:clean": "rimraf node_modules yarn.lock && yarn install --frozen-lockfile"
  },
  "build": {
    "appId": "com.example.svg2gif",
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    }
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "electron-is-dev": "2.0.0",
    "gifencoder": "2.0.1",
    "pngjs": "6.0.0",
    "node-webpmux": "3.0.0"
    "sharp": "0.33.2"
  },
  "devDependencies": {
    "electron": "26.0.0",
    "electron-builder": "24.6.0",
    "rimraf": "5.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  }
}
